    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: CommuteTube All",
            "type": "python",
            "request": "launch",
            "pythonPath": "~/.pyenv/commutetubepy3/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "module": "commute_tube",
            "args": [
                "--debug",
                "--config",
                "config.example.json"
            ],
            "program": "${file}"
        },
        {
            "name": "Python: CommuteTube Specific",
            "type": "python",
            "request": "launch",
            "pythonPath": "~/.pyenv/commutetubepy3/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "module": "commute_tube",
            "args": [
                "--debug",
                "--config",
                "config.example.json",
                "--filter",
                "Shellscript that lists youtube-dl urls line by line",
            ],
            "program": "${file}"
        }
    ]
}